<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/viewstudent"
    tools:context="com.example.saajidh.solutionseeker.ViewUser">

    <TextView
        android:id="@+id/textView53"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="19dp"
        android:layout_marginTop="17dp"
        android:text="Student Name"
        android:textColor="@android:color/black"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/txtviewStudentName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView53"
        android:layout_below="@+id/textView53"
        android:layout_marginStart="60dp"
        android:layout_marginTop="11dp"
        android:text="studentname"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView55"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView53"
        android:layout_below="@+id/txtviewStudentName"
        android:layout_marginTop="32dp"
        android:text="DCCN-I"
        android:textColor="@android:color/black"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView56"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView55"
        android:layout_below="@+id/textView55"
        android:layout_marginTop="22dp"
        android:text="IPE"
        android:textColor="@android:color/black"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView57"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView56"
        android:layout_below="@+id/textView56"
        android:layout_marginTop="23dp"
        android:text="MIT"
        android:textColor="@android:color/black"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView58"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView57"
        android:layout_below="@+id/textView57"
        android:layout_marginTop="22dp"
        android:text="CF"
        android:textColor="@android:color/black"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView59"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ST-I"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        android:layout_alignBaseline="@+id/textView55"
        android:layout_alignBottom="@+id/textView55"
        android:layout_toEndOf="@+id/txtviewStudentName"
        android:layout_marginStart="20dp" />

    <TextView
        android:id="@+id/textView60"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView59"
        android:layout_alignTop="@+id/textView56"
        android:text="DBMS-I"
        android:textColor="@android:color/black"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView61"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView57"
        android:layout_alignStart="@+id/textView60"
        android:text="ITA"
        android:textColor="@android:color/black"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView62"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView61"
        android:layout_alignTop="@+id/textView58"
        android:text="FCS"
        android:textColor="@android:color/black"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/txtviewDCCN1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/txtviewStudentName"
        android:layout_alignTop="@+id/textView59"
        android:layout_marginStart="21dp"
        android:text="dccn1"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/txtviewIPE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/txtviewDCCN1"
        android:layout_alignTop="@+id/textView60"
        android:text="ipe"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/txtviewMIT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView61"
        android:layout_alignStart="@+id/txtviewIPE"
        android:text="mit"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/txtviewCF"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView62"
        android:layout_alignStart="@+id/txtviewMIT"
        android:text="cf"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/txtviewST1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView59"
        android:layout_marginStart="16dp"
        android:layout_toEndOf="@+id/textView60"
        android:text="st1"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/txtviewDBMS1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/txtviewST1"
        android:layout_alignTop="@+id/textView60"
        android:text="dbms1"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/txtviewITA"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView61"
        android:layout_alignStart="@+id/txtviewDBMS1"
        android:text="ita"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/txtviewFCS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/txtviewITA"
        android:layout_alignTop="@+id/textView62"
        android:text="fcs"
        android:textSize="18sp" />

    <Button
        android:id="@+id/btnMessage"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="79dp"
        android:layout_toEndOf="@+id/textView60"
        android:text="Message"
        android:textAllCaps="false" />
</RelativeLayout>
